<!-- Gallery -->
<div class="container col-md-9 pe-md-5">
  <h1 class="display-4 text-center mb-4">Bangkok</h1>

  <div class="img-container-single mb-5">
    <img
      class="location-img w-100"
      src="https://res.cloudinary.com/davlk6kty/image/upload/v1721057284/Bangkok-sm-1_sgl6mw.jpg"
      alt="Bikes rushing past in Bangkok"
    />
    <p class="text-muted mt-2">Bikes rushing past in Bangkok</p>
  </div>

  <div class="img-container-single mb-5">
    <img
      class="location-img w-100"
      src="https://res.cloudinary.com/davlk6kty/image/upload/v1721057273/Bangkok-sm-3_ytlhps.jpg"
